	<build>
		<plugins>
			<plugin>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-maven-plugin</artifactId>
				<configuration>
					<mainClass>com.schoolOfDesign.jforce.Application</mainClass>
				</configuration>
			</plugin>
